{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Leads tracker",
    "commands": {
        "_execute_action": {
            "suggested_key": "CTRL+F",
            "description": "My first command"
        }
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab","tabs"
    ]
}